<!DOCTYPE html>
<html lang="es">

    <head>

        {{ app.environment == 'prod' ? include('default/gtm-head.html.twig') }}
        {{ app.environment == 'prod' ? include('ads/auto-ads.html.twig') }}

        <meta charset="utf-8">
        <link rel="icon" type="image/x-icon" href="{{ asset('img/favicon.ico') }}" />

        {% block seo %}
        <title>{% block title %}{{ seo.title }} - {{ seo.site }}{% endblock %}</title>
        <meta name="description" content="{{ seo.description }}">
        <script type='application/ld+json'>
        {
            "@context": "http://www.schema.org",
            "@type": "WebSite",
            "name": "{{ seo.name }}",
            "alternateName": "{{ seo.name }}",
            "url": "{{ seo.url }}",
            "description": "{{ seo.description }}"
        }
        </script>
        <link rel="canonical" href="{{ url('homepage') }}"/>
        {% endblock %}

        <meta name="robots" content="all">
        <meta name="googlebot" content="all">
        <link rel="alternate" href="{{ url('homepage') }}" hreflang="es_CO" />

        {% block opengraph %}
        {{ include('default/opengraph.html.twig', {title: seo.title, description: seo.description, img: seo.img, imgalt: seo.name}) }}
        {% endblock %}
        
        {% block twittercard %}
        {{ include('default/twittercard.html.twig', {title: seo.title, description: seo.description, img: seo.img, imgalt: seo.name}) }}
        {% endblock %}
        
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="author" content="{{ seo.author }}">

