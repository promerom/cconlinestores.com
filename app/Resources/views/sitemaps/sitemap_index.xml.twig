<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for url in urls %}
        <sitemap>
            <loc>
            {% if url.loc|replace({hostname:''}) == url.loc %}
                {{ hostname }}{{ url.loc }}
            {% else %}
                {{ url.loc }}
            {% endif %}
            </loc>
            <lastmod>{{ url.lastmod|date("d/m/Y g:ia") }}</lastmod>
        </sitemap>
    {% endfor %}
</sitemapindex>
